<?xml version="1.0" encoding="utf-8"?>
<Containers>
	<Date>2017-05-28</Date>
	<Beta>False</Beta>
	<Category>DatabaseServer:</Category>
	<Name>SQLServer</Name>
	<Project>https://github.com/laureanofo/docker-templates</Project>
	<Changes>
		[font
		size=3]2017.05.28[/font]
		- Initial Release
	</Changes>
	<Overview>
		SQL Server
	</Overview>
	<Description>
		SQL Server from centurylink
	</Description>
	<Registry>https://github.com/laureanofo/docker-templates</Registry>
	<GitHub>https://github.com/laureanofo/docker-templates</GitHub>
	<Repository>centurylink/mysql</Repository>
	<Support>https://github.com/laureanofo/docker-templates
	</Support>
	<BindTime>true</BindTime>
	<Privileged>true</Privileged>
	<Networking>
		<Mode>bridge</Mode>
		<Publish>
			<Port>
				<HostPort>3306</HostPort>
				<ContainerPort>3306</ContainerPort>
				<Protocol>tcp</Protocol>
			</Port>
		</Publish>
	</Networking>
	<Environment>
		<Variable>
			<Name>MYSQL_ROOT_PASSWORD</Name>
			<Value>password</Value>
		</Variable>
		<Variable>
			<Name>MYSQL_DATABASE</Name>
			<Value>moodle</Value>
		</Variable>
		<Variable>
			<Name>MYSQL_USER</Name>
			<Value>moodle</Value>
		</Variable>
		<Variable>
			<Name>MYSQL_PASSWORD</Name>
			<Value>moodle</Value>
		</Variable>
	</Environment>
	<Data>
		<Volume>
			<HostDir>/mnt/user/appdata/SQLServer</HostDir>
			<ContainerDir>/var/lib/mysql</ContainerDir>
			<Mode>rw</Mode>
		</Volume>
	</Data>
	<Banner></Banner>
	<Icon>https://www.mysql.com/common/logos/logo-mysql-170x115.png</Icon>
</Containers>