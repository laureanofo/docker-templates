<?xml version="1.0" encoding="utf-8"?>
<Containers>
	<Date>2017-05-28</Date>
	<Beta>False</Beta>
	<Category>DatabaseServer:</Category>
	<Name>Sql</Name>
	<Project>https://plex.tv/</Project>
	<Changes>
		[font
		size=3]2017.05.28[/font]
		- Initial Release
	</Changes>
	<Overview>
		SQL
	</Overview>
	<Description>
		SQL Server from centurylink
	</Description>
	<Registry>https://registry.hub.docker.com/u/limetech/plex/</Registry>
	<GitHub>https://github.com/limetech/dockerapp-plex</GitHub>
	<Repository>centurylink/mysql</Repository>
	<Support>http://lime-technology.com/forum/index.php?topic=40654.0
	</Support>
	<BindTime>true</BindTime>
	<Privileged>true</Privileged>
	<Networking>
		<Mode>bridge</Mode>
		<Publish>
			<Port>
				<HostPort>3306</HostPort>
				<ContainerPort>3306</ContainerPort>
				<Protocol>tcp</Protocol>
			</Port>
		</Publish>
	</Networking>
	<Environment>
		<Variable>
			<Name>MYSQL_ROOT_PASSWORD</Name>
			<Value>password</Value>
		</Variable>
	</Environment>
	<Data>
		<Volume>
			<HostDir>/mnt/user/appdata/SQLServer</HostDir>
			<ContainerDir>/config</ContainerDir>
			<Mode>rw</Mode>
		</Volume>
	</Data>
	<WebUI>http://[IP]:[PORT:32400]/web/index.html</WebUI>
	<Banner></Banner>
	<Icon>http://d2631fvi2jdnb6.cloudfront.net/docker/limetech/plex.png
	</Icon>
</Containers>